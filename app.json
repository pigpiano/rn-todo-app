{
  "expo": {
    "name": "rn-todo-app",
    "slug": "rn-todo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/free-icon-alien-head-by-Freepik.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/free-icon-alien-head-by-Freepik.png",
      "resizeMode": "contain",
      "backgroundColor": "#778bdd"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.pigpiano.rntodoapp",
      "buildNumber": "1.0.0",
      "supportsTablet": false,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "The app accesses to photo library to upload your profile when you are signing up or updating profile."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
